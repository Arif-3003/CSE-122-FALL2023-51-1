#include<bits/stdc++.h>
using namespace std;
class show
{
public:
      int x1,x2,x3;
      public:
      void input()
      {
      cin>>x1>>x2>>x3;
      }
      void output()
      {
          cout<<max(x1,max(x2,x3))<<endl;
      }
};
class floatshow : public show
{
public:
       float y1,y2,y3;
      void input()
      {
            show::input();
            cin>>y1>>y2>>y3;
      }
      void output()
      {
            show::output();
            if(y1>y2&&y1>y3)
            {
                  cout<<y1<<endl;
            }
           else  if(y2>y3&&y2>y1)
            {
                  cout<<y2<<endl;
            }
            else  if(y3>y2&&y3>y1)
            {
                  cout<<y3<<endl;
            }
      }
};
class charshow : public floatshow
{
public:
      char c1,c2,c3;
      void input()
      {

           floatshow::input();
            cin>>c1>>c2>>c3;
      }
      void output()
      {

           floatshow::output();
            if(c1>c2&&c1>c3)
            {
                  cout<<c1<<endl;
            }
            else if(c2>c3&&c2>c1)
            {
                  cout<<c2<<endl;
            }
            else if(c3>c2&&c3>c1)
            {
                  cout<<c3<<endl;
            }
      }
};
int main()

{
      charshow obj,obj1;
      obj.input();
      obj.output();
}
